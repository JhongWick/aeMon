@model Entities.Farms

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="col-md-3">
</div>
<div class="col-md-6">
    <div class="box box-danger">
        <div class="box-header">
            <h3 class="box-title">Delete</h3>
            <a href="@Url.Action("Index", "Farms")" class="btn btn-warning pull-right"><i class="fa fa-list"></i>&nbsp;&nbsp;Back to List</a>
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <table id="example1" class="table table-bordered table-striped">

                <tbody>

                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.RegionId)
                        </th>

                        <td>
                            @DataAccessLayer.RegionData.returnName(@Model.RegionId)
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Barangay)
                        </th>

                        <td>
                            @Html.DisplayFor(model => model.Barangay)
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Municipality)
                        </th>

                        <td>
                            @Html.DisplayFor(model => model.Municipality)
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Province)
                        </th>

                        <td>
                            @Html.DisplayFor(model => model.Province)
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.completeAddress)
                        </th>

                        <td>
                            @Html.DisplayFor(model => model.completeAddress)
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Longititude)
                        </th>

                        <td>
                            @Html.DisplayFor(model => model.Longititude)
                        </td>
                    </tr>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.Latitude)
                        </th>

                        <td>
                            @Html.DisplayFor(model => model.Latitude)
                        </td>
                    </tr>

                </tbody>
            </table>
            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()
                <div class="form-actions no-color" style="padding-top:20px !important;">
                    <input type="submit" value="Delete" class="btn btn-danger pull-left" />
                </div>
            }

        </div>
    </div>
    <!-- /.box -->
</div>
<div class="col-md-3">
</div>

