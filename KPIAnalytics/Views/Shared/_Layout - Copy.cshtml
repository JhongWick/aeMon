    <link href="~/Content/bootswatch/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootswatch/bootstrap.min.css" rel="stylesheet" />
    <link href="~/select2/css/select2.css" rel="stylesheet" />
    <link href="~/Content/vendor/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/ProcurementLayout2.css" rel="stylesheet" />*@

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@
    @RenderSection("scripts", false)
</body>
</html>
